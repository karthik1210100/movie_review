<%= form_with method: :get, class: 'form-inline' do |form| %>
  <div class="form-group">
    <%= form.date_field :start_date, class: 'form-control', required: true, placeholder: "search" %>
  </div>
  <div class="form-group ml-3">
    <%= form.date_field :end_date, class: 'form-control', required: true %>
  </div>
  <%= form.submit "Show detail", class: 'btn btn-secondary ml-3' %>
<% end %>